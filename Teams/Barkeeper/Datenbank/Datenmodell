INSERT Beispiel:

    -- Cocktail einf端gen
    INSERT INTO cocktails_t (name, beschreibung)
    VALUES ('Cocktail', 'Ein Cocktail');

    -- Zutat einf端gen
    INSERT INTO zutat_t (name, beschreibung)
    VALUES ('Zutat', 'Eine Zutat');

    -- Relation einf端gen 端ber cocktail_id und zutat + Menge
    INSERT INTO Rezept_RT(COCKTAILS_ID, Zutat, Menge)
    VALUES (1, 'Zutat', '1 cl')